      uet('x6', 'ewc', {wb: 1});
      ue.count('ewc:latency:jquery', new Date()-ue.t0);
      recordedJqueryReadyTime = true;
    };
    (window.AmazonUIPageJS ? AmazonUIPageJS : P).when('jQuery').execute(function() {
      recordJqueryReadyTime();
    });
    if (window.$Nav) {
        window.$Nav.when('$').run(function() {
            recordJqueryReadyTime();
        });
    }
  }());
}
