        <div class="threepsl-creative" data-index="2"></div>
    </div>

    <div class="threepsl-footer">
        <span class="threepsl-ad-feedback"></span>
    </div>
</div>

<script type="text/javascript">
  (function() {
    var root = document.getElementById('affc4414-dfbf-4da3-a698-85aead15f208');
    var creativeType = 'MultiBrandCreativeDesktop';
    var triggerThirdPixelMediaQuery = {
      MultiBrandCreativeDesktop: '(min-width: 1550px)',
      MultiBrandLifestyleCreativeDesktop: '(min-width: 1001px)'
    };

    var pixels = {
      first: '//aax-us-iad.amazon.com/e/loi/imp?b=Ipig3858crYqH9-9GbOhg2EAAAFxQXgF_wEAAAH2AWFTZ6o',
      second: '//aax-us-iad.amazon.com/e/loi/imp?b=InqwBYtBYfdBCrTsO91RGv0AAAFxQXgF-AEAAAH2AXiDU2w',
      third: ''
    };

    window.addEventListener('DOMContentLoaded', p13nHandler);
    window.addEventListener('load', p13nHandler);
    window.addEventListener('scroll', p13nHandler);
    window.addEventListener('DOMContentLoaded', impressionAndFeedbackHandler);

    function impressionAndFeedbackHandler() {
      firePixel(pixels.first);
      firePixel(pixels.second);

      handleThirdCreative();

      loadAdFeedback();
    }

    function p13nHandler() {
      var creativesEl = root.querySelector(".threepsl-creatives");
      if (isElementInViewport(creativesEl)) {
        var p13n_imp_url = createP13NImpUrl();
        firePixel(p13n_imp_url);

        window.removeEventListener('DOMContentLoaded', p13nHandler);
        window.removeEventListener('load', p13nHandler);
        window.removeEventListener('scroll', p13nHandler);
      }
    }

    /**
     * A general example format for impression with global metadata: https://tiny.amazon.com/djxp11em/wamazbinviewClicUserImprSen
     * fls-na.amazon.com/1/my-channel/1/OP/my-program-group/impression-type/feature1:resource1,resource2?requestId=rid&marketplaceId=mid&session=sid
     *
     * More specific url format in p13n-logger.js https://tiny.amazon.com/uxufhebi
     */
    function createP13NImpUrl() {
      var P13N_FORESTER_CHANNEL_NAME = 'p13n-shared-components';
      var PERCOLATE_IMP_DATA = 'action/sb_detail:ad1,ad2:action=view,pd_rd_w=FFxCt,pf_rd_p=4fc0c6cc-884d-4533-9cdc-ddf18e23ce32,pd_rd_wg=HcfhB,pf_rd_r=MAGN5P4Q4SA4111YWF0W,pd_rd_r=21780230-2927-4540-8c5a-4c90341bdd54?marketplaceId=ATVPDKIKX0DER&requestId=21780230-2927-4540-8c5a-4c90341bdd54&session=144-4422790-7680220';

      return [
        document.location.protocol + '//',
        window.ue_furl || 'fls-na.amazon.com',
        '/1/',
        P13N_FORESTER_CHANNEL_NAME,
        '/1/OP/p13n/',
        PERCOLATE_IMP_DATA
      ].join('');
    }

    function firePixel(url) {
      if (url && url.length) {
        new Image().src = url;
      }
    }

    function handleThirdCreative() {
      var query = triggerThirdPixelMediaQuery[creativeType];
      if (!window.matchMedia || !pixels.third || !pixels.third.length || !query) return;

      var mediaQuery = window.matchMedia(query);
      if (mediaQuery.matches) {
        firePixel(pixels.third);
      } else {
        mediaQuery.addListener(onWindowSizeChange);
      }

      function onWindowSizeChange(e) {
        if (e.matches) {
          firePixel(pixels.third);
          mediaQuery.removeListener(onWindowSizeChange);
        }
      }
    }

    function isElementInViewport(el) {
      var rect = el.getBoundingClientRect();
      /**
       * Reverse thinking here:
       *
       * If an element is OUT of viewport:
       * 1) It’s position from the bottom will always be less than or equal to 0.
       * 2) OR It’s position from the right will always be less than or equal to 0.
       * 3) OR It’s distance from the top will be greater than or equal to the total height of the viewport
       * 4) OR it’s distance from the left will be greater than or equal to the width of the viewport.
       */
      return !(
        rect.top >= window.innerHeight ||
        rect.left >= window.innerWidth ||
        rect.bottom <= 0 ||
        rect.right <= 0
      );
    }

    function loadAdFeedback() {
      var element = root.querySelector('.threepsl-ad-feedback');

      if (element) {
        var TEMPLATE_ID = 'desktopFooter';
        var AD_FEEDBACK_URL = '/gp/aq-feedback/lazyLoad/handler/af-link-handler.html';
        var AD_PROGRAM_ID = 1010;
        var AD_NETWORK = 'aax';
        var PAGE_TYPE = 'Detail';
        var SLOT_NAME = 'desktop-arbies';
        var SEARCH_TERMS = 'SEARCH_KEYWORDS';

        var encodedSearchTerms = '';
        if (typeof window.btoa !== 'undefined' && SEARCH_TERMS) {
          encodedSearchTerms = btoa(
            SEARCH_TERMS
              .replace(
                /%([0-9A-F]{2})/g,
                function toSolidBytes(match, p1) { return String.fromCharCode('0x' + p1); }
              )
          );
        }

        var requestParamsJson = JSON.stringify({
          adPlacementMetaData: {
            adElementId: TEMPLATE_ID,
            pageType: PAGE_TYPE,
            slotName: SLOT_NAME, /* required */
            pageUrl: '',
            searchTerms: encodedSearchTerms
          },
          adCreativeMetaData: {
            adNetwork: AD_NETWORK,
            adProgramId: AD_PROGRAM_ID, /* required */
            adImpressionId: '',
            adCreativeId: '',
            adId: ''
          }
        });

        var requestUrl = window.location.protocol
          + '//'
          + window.location.hostname
          + AD_FEEDBACK_URL
          + '?pl='
          + encodeURIComponent(requestParamsJson);

        var xmlHttpRequest = new XMLHttpRequest();

        xmlHttpRequest.onreadystatechange = function() {
          if (xmlHttpRequest.readyState !== 4 || xmlHttpRequest.status !== 200) {
            return;
          }

          var ajaxResponse = xmlHttpRequest.responseText;
          var result = JSON.parse(ajaxResponse);

          if (result && result.status === 'ok') {
            if (('html' in result) && result.html) {
              element.innerHTML = result.html;
            }

            if (('script' in result) && result.script) {
              var scriptWrapper = document.createElement('script');
              scriptWrapper.type = 'text/javascript';
              scriptWrapper.innerHTML = result.script;
              element.appendChild(scriptWrapper);
            }
          }
        };

        xmlHttpRequest.open("GET", requestUrl, true);
        xmlHttpRequest.send();
      }
    }
  })();
</script>
</div>



