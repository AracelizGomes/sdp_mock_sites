    window.$Nav && $Nav.when('$', 'page.ready').run('MXMarketplaceRedirectOverlay',function($) {
        $.ajax({
            type: 'POST',
            url: '/gp/redirection/mexico.html',
            data: {
                path: '/LOTUS-FOODS-Organic-White-Noodles/dp/B07FQX181T',
                queryString: '?ie=UTF8\u0026keywords=708953641027\u0026qid=1585941304\u0026sr=8-1\u0026dchild=1',
                pageType: 'Detail',
                referer: 'https://www.amazon.com/LOTUS-FOODS-Organic-White-Noodles/dp/B07FQX181T/ref=sr_1_1?dchild=1\u0026keywords=708953641027\u0026qid=1585941304\u0026sr=8-1'
            },
            success: function(data) {
