&document.documentElement&&"ontouchstart"in document.documentElement))return!1;for(var n=function(e){var n=Math.abs(e);return n>A&&(t.swipe({direction:e<0?"left":"right",length:n}),!0)},r=e.querySelectorAll("[data-track-swipe]"),i=0;i<r.length;++i)L(r[i],n)}(e,n),function(e,t){for(var n=e.querySelectorAll("img"),r=n.length,i=0,o=function(){++i===r&&t.visualCompleteness()},a=0;a<r;++a)y(n[a],o)}(e,n),P(e,n)}n.d(t,"attach",function(){return k}),n.d(t,"logError",function(){return j});var j=function(e){window._basebe&&window._basebe.nori&&window._basebe.nori.jsError&&window._basebe.nori.jsError(e)};function C(e){var t=e.getAttribute("data-weblab-id");return t&&t.length?t.split(","):null}function M(e){var t=e.querySelectorAll("[data-asin]");return t&&t.length?[].map.call(t,function(e){return{id:e.dataset.asin,prime:T(e,"prime"),price:T(e,"price"),savings:T(e,"savings"),rating:q(e,"rating"),badge:q(e,"badge")}}):null}function T(e,t){return null!==e.querySelector('[data-click-el="'.concat(t,'"]'))}function q(e,t){var n=e.querySelector("[data-".concat(t,"]"));return n?n.getAttribute("data-".concat(t)):null}}}); metrics.attach(root, {"anonymizedRequestId":"0101b37c1dbd310680b080a88d3c592fa3cf7fd3cffa93b5809cafac995e011afc47","metrics":{"sampleRate":0.01,"enabled":true},"errors":{"sampleRate":0.01,"enabled":true},"slotName":"desktop-arbies","market":"US"}); function onScriptReady() { try {  runMultiBrandCreativeDesktopClient(root, {"lazyloadDelay":null}); } catch (ex) { if (typeof metrics !== 'undefined') { metrics.logError(ex); } }}var initialize = (function() { var initialized = false; return function loadScript() { if (initialized) return; initialized = true;  var script = document.createElement('script'); script.setAttribute('crossorigin', 'anonymous'); script.setAttribute('src', 'https://images-na.ssl-images-amazon.com/images/G/01/dacx/sb/6kA6cOJY6nOsiphbF7eBH0gdojU/MultiBrandCreativeDesktop.client.bundle.js'); script.onload = onScriptReady; document.body.appendChild(scr